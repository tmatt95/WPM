{% extends 'parts.html.twig' %}

{% block title %}Parts: View{% endblock %}

{% block body %}
    <h1>View Part - </h1>
    <ol class="breadcrumb">
        <li><a href="{{ path('parts_find') }}">Parts</a></li>
        <li class="active">View Part</li>
    </ol>
    {% if displayMessage.value != '' %}
    <div class="row">
        <div class="col-xs-12">
            <div class="alert {{displayMessage.class}}" role="alert">
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                {{displayMessage.value}}
            </div>
        </div>
    </div>
{% endif %}

    {{ form_start(form) }}
    {{ form_widget(form) }}
    {{ form_end(form) }}

    <h3>Description</h3>


    <h3>Quatity</h3>


    <h3>Auditing</h3>
{% endblock %}
